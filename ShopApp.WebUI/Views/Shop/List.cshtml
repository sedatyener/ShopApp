@model ShopApp.WebUI.Models.ProductListModel

<div class="row mt-3">
    <div class="col-md-3">
        <ul class="list-group">
            <li class="list-group-item">Telefon</li>
            <li class="list-group-item">Bilgisayar</li>
            <li class="list-group-item">Elektornik</li>
        </ul>
    </div>
    <div class="col-md-9">
       
        <div class="row">
            @foreach (var product in Model.Products)
            {
                <partial name="_product" for="@product" />
            }
        </div>
    </div>
</div>